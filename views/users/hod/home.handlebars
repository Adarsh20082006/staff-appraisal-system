        <span class="card-title">{{name}}</span>
        <p>Email ID: {{email}}</p>
        <p>Employee Type: {{type}}</p>
      </div>
      <div class="card-action">
        <a href="/users/hod/hodOverview/{{_id}}" class="purple-text">Faculty Feedback Form</a><br><br>
          <a href="/users/hod/confidential" class="purple-text">Confidential Form</a>
      </div>
    </div>
  </div>

