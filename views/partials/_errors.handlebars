{{#if error}}
    <div class="row" id="alert_box">
    <div class="col s12 m12">
        <div class="card red lighten-1">
            <div class="row">
                <div class="col s12 m10">
                    <div class="card-content white-text">
                        <p>{{error}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/if}}

{{#each errors}}
<div class="row" id="alert_box">
    <div class="col s12 m12">
        <div class="card red lighten-1">
            <div class="row">
                <div class="col s12 m10">
                    <div class="card-content white-text">
                        <p>{{text}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{else}}


{{/each}}