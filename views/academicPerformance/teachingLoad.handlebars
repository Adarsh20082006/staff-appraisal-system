<h4> Academic Performance</h4><br>
<div class="container">
<h5> Teaching Load</h5>
<hr>

{{#if result}}
{{#each result}}
    <div class="row" id="leaveform">
        <div class="row">
            <div class="input-field col s6">
                <select name="academic_year">
                    <option value="{{academic_year}}">{{academic_year}}</option>
                    <option value="2014-15" disabled>2014-15</option>
                    <option value="2015-16" disabled>2015-16</option>
                    <option value="2016-17" disabled>2016-17</option>
                    <option value="2017-18" disabled>2017-18</option>
                    <option value="2018-19" disabled>2018-19</option>
                    <option value="2019-20" disabled>2019-20</option>
                </select>
                <label for="">Academic Year</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6 md5">
                <input id="subject_name" type="text" name="subject_name" value="{{subject_name}}" disabled>
                
                <label for="subject_name" class="">Subject Name</label>
                
            </div>
            <div class="input-field col s6">
                <select name="class">
                    <option value="{{class}}">{{class}}</option>
                    <option value="FE" disabled>FE</option>
                    <option value="SE" disabled>SE</option>
                    <option value="TE" disabled>TE</option>
                    <option value="BE" disabled>BE</option>
                </select>
                <label for="">Class</label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s6">
                <select name="department">
                    <option value="{{department}}">{{department}}</option>
                    <option value="Information Technology" disabled>Information Technology</option>
                    <option value="Computer Engineering" disabled>Computer Engineering</option>
                    <option value="Mechanical Engineering" disabled>Mechanical Engineering</option>
                    <option value="Civil Engineering" disabled>Civil Engineering</option>
                    <option value="EXTC Engineering" disabled>EXTC Engineering</option>
                </select>
                <label for="">Department</label>
            </div>
            <div class="input-field col s6 md5">
                <select name="semester">
                    <option value="{{semester}}">{{semester}}</option>
                    <option value="sem 1" disabled>Sem 1</option>
                    <option value="sem 2" disabled>Sem 2</option>
                    <option value="sem 3" disabled>Sem 3</option>
                    <option value="sem 4" disabled>Sem 4</option>
                    <option value="sem 5" disabled>Sem 5</option>
                    <option value="sem 6" disabled>Sem 6</option>
                    <option value="sem 7" disabled>Sem 7</option>
                    <option value="sem 8" disabled>Sem 8</option>
                </select>
                <label for="">Semester</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6 md5">
                <input id="theory" type="number" name="theory_subject" value="{{theory_subject}}" disabled>
                 <label for="theory">Theory Load &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Theory Teaching Load per week"><i class="fa fa-info-circle"></i></a></label>
            </div>
            <div class="input-field col s6">
                <input id="lab" type="number" name="lab_subject" value="{{lab_subject}}" disabled>
                 <label for="lab">Lab Load &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Lab Teaching Load per week"><i class="fa fa-info-circle"></i></a></label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s6">
                <input id="tutorials" type="number" name="tutorials" value="{{tutorials}}" disabled>
                <label for="tutorials">Tutorials Load &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Tutorials Teaching Load per week"><i class="fa fa-info-circle"></i></a></label>
            </div>
            <div class="input-field col s6">
                <input id="theory_session" type="number" name="theory_session" value="{{theory_session}}" disabled>
               <label for="theory_session">Theory sessions &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="No. of Theory sessions conducted (hours)"><i class="fa fa-info-circle"></i></a></label>
                
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input id="practical_session" type="number" name="practical_session" value="{{practical_session}}" disabled>
                <label for="practical_session">Practical sessions &nbsp; <a href="#" class="tooltipped"
                data-tooltip="No. of Practical sessions conducted (hours)"><i class="fa fa-info-circle"></i></a></label>
            </div>
            <div class="input-field col s6">
                <input id="Student_feedback" type="number" name="Student_feedback" value="{{Student_feedback}}" disabled>
               <label for="Student_feedback">Feedback &nbsp; <a href="#" class="tooltipped"
                data-tooltip="Online Students Feedback (1-5)"><i class="fa fa-info-circle"></i></a></label>
                
            </div>
        </div>
        <a href="/academicPerformance/teachingLoad/edit/{{id}}" class="btn waves-effect waves-light red center mb-2">Edit</a>
        <a href="/users/faculty/facultyOverview" class="btn waves-effect waves-light purple center mb-2">Back to overview</a><br><br>
        <form method="post" action="/academicPerformance/teachingLoad/delete/{{id}}?_method=DELETE">
            <input type="hidden" name="_method" value="DELETE">
            <input type="submit" class="btn waves-effect waves-light red center" value="Delete">
        </form>
        
</div>
</div>
{{else}}
{{/each}}
